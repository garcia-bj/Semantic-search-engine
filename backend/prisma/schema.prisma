generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Document {
  id          String   @id @default(uuid())
  filename    String
  filePath    String?
  tripleCount Int      @default(0)
  createdAt   DateTime @default(now())

  @@map("documents")
}
